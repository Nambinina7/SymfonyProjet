<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}{{ encore_entry_link_tags('app') }}{% endblock %}
    </head>
    <body>
        {% block body %}{% endblock %}
        {% block javascripts %}
            <script>
                var isAdmin = false
                {% if is_granted('ROLE_ADMIN') %}
                    isAdmin = true
                {% endif %}
            </script>
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </body>
</html>
